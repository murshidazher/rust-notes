{
  "name": "snake-game-wasm",
  "version": "0.0.1",
  "description": "A rust wasm based snake game",
  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint --cache --fix '**/*.js'",
    "lint:prettier:fix": "prettier --write '**/*.js'",
    "prebuild": "npm run build:rust && pnpm i",
    "build": "webpack build --config webpack.prod.js",
    "build:rust": "wasm-pack build --target web",
    "predev": "npm run build:rust && pnpm i",
    "dev": "webpack-dev-server --hot --config webpack.dev.js"
  },
  "keywords": [
    "wasm",
    "rust",
    "snake-game"
  ],
  "author": "Murshid Azher <hello@murshidazher.com>",
  "license": "MIT",
  "dependencies": {
    "snake_game": "file://../pkg"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0-alpha.1",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "pnpm run lint:fix"
    ]
  }
}
